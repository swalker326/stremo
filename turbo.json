{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [
    ".env"
  ],
  "globalDependencies": [
    "**/.env.*local",
    "**/.env.global"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "db#db:generate"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {},
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}